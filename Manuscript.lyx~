# 220    2        1         304.89200
\end_layout

\begin_layout Plain Layout

# 220    2        2         302.63036
\end_layout

\begin_layout Plain Layout

CreateDataRedox <- function()
\end_layout

\begin_layout Plain Layout

{	
\end_layout

\begin_layout Plain Layout

	data_redox <- read.table("Redox.csv",header=TRUE,sep="
\backslash
t")
\end_layout

\begin_layout Plain Layout

	data_redox <- subset(data_redox,depth_cm != 5)
\end_layout

\begin_layout Plain Layout

	data_redox
\end_layout

\begin_layout Plain Layout

	#Remove the replicate column
\end_layout

\begin_layout Plain Layout

	data_redox <- data_redox[c("dist_m","depth_cm","redox_calib") ]
\end_layout

\begin_layout Plain Layout

	# Take the average redox values, so that every distance and depth has a
 single redox value
\end_layout

\begin_layout Plain Layout

	data_redox <- aggregate(data_redox,list(data_redox$depth_cm,data_redox$dist_m),
mean)
\end_layout

\begin_layout Plain Layout

	data_redox <- data_redox[c("dist_m","depth_cm","redox_calib") ]
\end_layout

\begin_layout Plain Layout

	data_redox
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

