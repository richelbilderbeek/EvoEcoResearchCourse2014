\begin_layout Plain Layout

TallySpeciesPerRedox <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	redox_to_species <- dcast(
\end_layout

\begin_layout Plain Layout

		GetDataCombined(),
\end_layout

\begin_layout Plain Layout

		redox_calib ~ species_name,
\end_layout

\begin_layout Plain Layout

		value.var = "species_name",
\end_layout

\begin_layout Plain Layout

		fill = 0, 
\end_layout

\begin_layout Plain Layout

		fun.aggregate=length # fun.aggregate may also be mean
\end_layout

\begin_layout Plain Layout

	) 
\end_layout

\begin_layout Plain Layout

	redox_calib <- redox_to_species$redox_calib
\end_layout

\begin_layout Plain Layout

	# Note that not all species are present anymore that often.
 This is due to that not all distances are redoxed
\end_layout

\begin_layout Plain Layout

	redox_to_species
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

TallySelectedSpeciesPerRedox <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	redox_to_species <- TallySpeciesPerRedox()
\end_layout

\begin_layout Plain Layout

	redox_to_species <- redox_to_species[, colSums(redox_to_species) > 6]
\end_layout

\begin_layout Plain Layout

	redox_to_species <- cbind(TallySpeciesPerRedox(),redox_to_species)
\end_layout

\begin_layout Plain Layout

	redox_to_species <- redox_to_species[, c("redox_calib","Hydrobia_ulvae","Nereis
_diversicolor")]
\end_layout

\begin_layout Plain Layout

	redox_to_species
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Can the redox potentials at a certain distane be assumed to be linear
 at a certain depth?
\end_layout

\begin_layout Plain Layout

CalcOrderednessPerDistance <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	orderedness_per_distance <- data.frame(dist_m = numeric(),order = factor())
\end_layout

\begin_layout Plain Layout

	for (i in GetDistances())
\end_layout

\begin_layout Plain Layout

	{
\end_layout

\begin_layout Plain Layout

    data <- subset(CreateDataRedoxAll(),dist_m == i)
\end_layout

\begin_layout Plain Layout

    low <- subset(data,depth_cm == 2)$redox_calib
\end_layout

\begin_layout Plain Layout

    mid <- subset(data,depth_cm == 5)$redox_calib
\end_layout

\begin_layout Plain Layout

    high <- subset(data,depth_cm == 10)$redox_calib
\end_layout

\begin_layout Plain Layout

	  if (low < mid && mid < high) 
\end_layout

\begin_layout Plain Layout

	  { 
\end_layout

\begin_layout Plain Layout

	  	orderedness_per_distance <- rbind(orderedness_per_distance,data.frame(dist_m
 = i,order = "LTH"))
\end_layout

\begin_layout Plain Layout

	  } else if(low > mid && mid > high) 
\end_layout

\begin_layout Plain Layout

	  { 
\end_layout

\begin_layout Plain Layout

	  	orderedness_per_distance <- rbind(orderedness_per_distance,data.frame(dist_m
 = i, order = "HTL"))
\end_layout

\begin_layout Plain Layout

	  } else  { 
\end_layout

\begin_layout Plain Layout

	  	orderedness_per_distance <- rbind(orderedness_per_distance,data.frame(dist_m
 = i, order = "UNO"))
\end_layout

\begin_layout Plain Layout

	  }
\end_layout

\begin_layout Plain Layout

	}
\end_layout

\begin_layout Plain Layout

	orderedness_per_distance
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Generate figure for species abundances for the range of redox potentials
\end_layout

\begin_layout Plain Layout

#   in two vertically aligned plots
\end_layout

\begin_layout Plain Layout

CreateFigureSpeciesAbundancesSeperate <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	svg(filename="Figure_species_abundances_seperate.svg")
\end_layout

\begin_layout Plain Layout

	par(mfrow=c(2,1))
\end_layout

\begin_layout Plain Layout

	plot(
\end_layout

\begin_layout Plain Layout

		Hydrobia_ulvae ~ redox_calib, 
\end_layout

\begin_layout Plain Layout

		data = TallySpeciesPerRedox(), 
\end_layout

\begin_layout Plain Layout

		t = "b", 
\end_layout

\begin_layout Plain Layout

		pch = 19, 
\end_layout

\begin_layout Plain Layout

		col = "black",
\end_layout

\begin_layout Plain Layout

		#main = "Hydrobia ulvae abundance",
\end_layout

\begin_layout Plain Layout

		xlab = "Redox potential (mV)",
\end_layout

\begin_layout Plain Layout

		ylab = "# Hydrobia ulvae"
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	plot(
\end_layout

\begin_layout Plain Layout

		Nereis_diversicolor ~ redox_calib, 
\end_layout

\begin_layout Plain Layout

		data = TallySpeciesPerRedox(), 
\end_layout

\begin_layout Plain Layout

		t = "b", 
\end_layout

\begin_layout Plain Layout

		pch = 19, 
\end_layout

\begin_layout Plain Layout

		col = "black",
\end_layout

\begin_layout Plain Layout

		#main = "Nereis diversicolor abundance",
\end_layout

\begin_layout Plain Layout

		xlab = "Redox potential (mV)",
\end_layout

\begin_layout Plain Layout

		ylab = "# Nereis Diversicolor",
\end_layout

\begin_layout Plain Layout

		ylim = c(0,4)
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	par(mfrow=c(1,1))
\end_layout

\begin_layout Plain Layout

	dev.off()
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Generate figure for species abundances for the range of redox potentials
\end_layout

\begin_layout Plain Layout

#   in the same plot
\end_layout

\begin_layout Plain Layout

CreateFigureSpeciesAbundances <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	svg(filename="Figure_species_abundances.svg")
\end_layout

\begin_layout Plain Layout

	par(mar = c(5, 4, 4, 4) + 0.3)  # Leave space for z axis
\end_layout

\begin_layout Plain Layout

	par(new = FALSE) 
\end_layout

\begin_layout Plain Layout

	plot(Hydrobia_ulvae ~ redox_calib, data = TallySpeciesPerRedox(), pch=19,
 axes=FALSE, xlab="Redox potential (mV)", ylab="", 
\end_layout

\begin_layout Plain Layout

	   type="b",col="blue"
\end_layout

\begin_layout Plain Layout

		#main="Species abundandances
\backslash
nfor different redox potentials"
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	axis(1, col="black",las=1) #'las=1' align labels horizontally
\end_layout

\begin_layout Plain Layout

	axis(2, col="blue",las=1) #'las=1' align labels horizontally
\end_layout

\begin_layout Plain Layout

	mtext("# Hydrobia",side=2,line=2.5,col="blue")
\end_layout

\begin_layout Plain Layout

	box()
\end_layout

\begin_layout Plain Layout

	par(new = TRUE) # Prevents R from clearing the area 
\end_layout

\begin_layout Plain Layout

	plot(
\end_layout

\begin_layout Plain Layout

		Nereis_diversicolor ~ redox_calib, axes = FALSE, bty = "n", xlab = "",
 ylab = "", data = TallySpeciesPerRedox(),
\end_layout

\begin_layout Plain Layout

		type="b",
\end_layout

\begin_layout Plain Layout

		col = "red",
\end_layout

\begin_layout Plain Layout

		yaxt="n", 
\end_layout

\begin_layout Plain Layout

		pch = 19
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	axis(
\end_layout

\begin_layout Plain Layout

		side=4, 
\end_layout

\begin_layout Plain Layout

		at=seq(0,3,1), #Otherwise, 0.5 would be shown as a tick mark
\end_layout

\begin_layout Plain Layout

		col="red",
\end_layout

\begin_layout Plain Layout

		las=1 #Align labels horizontally
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	mtext("# Nereis", side=4, line=3,col="red")
\end_layout

\begin_layout Plain Layout

	legend("topright",
\end_layout

\begin_layout Plain Layout

	  inset=0.05,title = "Species name", c("Hydrobia ulvae", "Nereis diversicolor"),
horiz=TRUE,
\end_layout

\begin_layout Plain Layout

	  fill=c("blue","red"), 
\end_layout

\begin_layout Plain Layout

		cex = 0.75
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	dev.off()
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Redox potentials along the transect for the two depths
\end_layout

\begin_layout Plain Layout

CreateFigureRedoxPerDistance <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	svg(filename="Figure_redox_per_distance.svg")
\end_layout

\begin_layout Plain Layout

	dist_to_redox <- subset(CreateDataRedox(), CreateDataRedox()$dist_m %in%
 GetDistances())
\end_layout

\begin_layout Plain Layout

	y_min <- min(dist_to_redox$redox_calib) - 100
\end_layout

\begin_layout Plain Layout

	y_max <- max(dist_to_redox$redox_calib) + 1000
\end_layout

\begin_layout Plain Layout

	y_text <- y_max - 200
\end_layout

\begin_layout Plain Layout

	plot(
\end_layout

\begin_layout Plain Layout

		dist_to_redox$redox_calib ~ dist_to_redox$dist_m,
\end_layout

\begin_layout Plain Layout

		col = as.factor(dist_to_redox$depth_cm), 
\end_layout

\begin_layout Plain Layout

		pch = 19,
\end_layout

\begin_layout Plain Layout

		#main="Redox potentials along the transect
\backslash
nfor the two depths",
\end_layout

\begin_layout Plain Layout

		xlab="Distance along transect (m)",
\end_layout

\begin_layout Plain Layout

		ylab="Redox potential (mV)",
\end_layout

\begin_layout Plain Layout

		ylim=c(y_min,y_max)
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	segments(1125,y_min,1125,y_max,col="black",lty=3)
\end_layout

\begin_layout Plain Layout

	segments(1900,y_min,1900,y_max,col="black",lty=3)
\end_layout

\begin_layout Plain Layout

	segments(2225,y_min,2225,y_max,col="black",lty=3)
\end_layout

\begin_layout Plain Layout

	text((1125 + 150) / 2,y_text,"Salt
\backslash
nmarsh")
\end_layout

\begin_layout Plain Layout

	text((1900 + 1125) / 2,y_text,"Mud
\backslash
nflat")
\end_layout

\begin_layout Plain Layout

	text((2225 + 1900) / 2,y_text,"Mussel
\backslash
nbed")
\end_layout

\begin_layout Plain Layout

	text(-50 + ((2600 + 2225) / 2),y_text,"Mud
\backslash
nflat")
\end_layout

\begin_layout Plain Layout

	legend("bottomleft",
\end_layout

\begin_layout Plain Layout

	  inset=0.05,title = "Sampling depth", c("2 cm", "10 cm"),horiz=TRUE,
\end_layout

\begin_layout Plain Layout

	  fill=c(1,2,3), cex = 1.0
\end_layout

\begin_layout Plain Layout

	)
\end_layout

\begin_layout Plain Layout

	rm(dist_to_redox)
\end_layout

\begin_layout Plain Layout

	rm(y_min)
\end_layout

\begin_layout Plain Layout

	rm(y_max)
\end_layout

\begin_layout Plain Layout

	rm(y_text)
\end_layout

\begin_layout Plain Layout

	dev.off()
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

assert("CreateDataBenthos: 863 individuals were scored at known depths",length(C
reateDataBenthos()$dist_m) == 863)
\end_layout

\begin_layout Plain Layout

assert("GetSpeciesCountAtDepths: All 863 individuals must be seperated correctly
 at their depths",length(CreateDataBenthos()$dist_m) == sum(GetSpeciesCountAtDep
ths()$"2") + sum(GetSpeciesCountAtDepths()$"10"))
\end_layout

\begin_layout Plain Layout

assert("GetSpeciesCountAtDepths: 20 species were scored at all depths",length(Ge
tSpeciesCountAtDepths()$species_name) == 20)
\end_layout

\begin_layout Plain Layout

assert("GetSelectedSpecies: 8 species were found at both depths, at each
 depth occurring at least thrice",length(GetSelectedSpecies()) == 8)
\end_layout

\begin_layout Plain Layout

assert("GetDataBenthosSelected: 740 individuals of the 8 selected species
 were scored at known depths",length(GetDataBenthosSelected()$species_name)
 == 740)
\end_layout

\begin_layout Plain Layout

assert("CreateDataRedox must only consider depths of 2 and 10 cm",length(subset(
CreateDataRedox(),depth_cm != 2 & depth_cm != 10)$depth_cm) == 0)
\end_layout

\begin_layout Plain Layout

assert("GetRedoxValues: 32 redox potentials are investigated",length(GetRedoxVal
ues()) == 32)
\end_layout

\begin_layout Plain Layout

assert("GetDistances: 16 distances are investigated",length(GetDistances())
 == 16)
\end_layout

\begin_layout Plain Layout

assert("GetDataCombined: needs to be 1176 Hydrobia",length(subset(GetDataCombine
d(),species_name == "Hydrobia_ulvae")$species_name) == 294)
\end_layout

\begin_layout Plain Layout

assert("GetDataCombined: needs to be 36 Nereis diversicolor",length(subset(GetDa
taCombined(),species_name == "Nereis_diversicolor")$species_name) == 9)
\end_layout

\begin_layout Plain Layout

assert("GetRedoxesHydrobia: must be 294 values",length(GetRedoxesHydrobia()$redo
x_calib) == 294)
\end_layout

\begin_layout Plain Layout

assert("GetRedoxesNereis: must be 9 values",length(GetRedoxesNereis()$redox_cali
b) == 9)
\end_layout

\begin_layout Plain Layout

assert("TallySpeciesPerRedox: must be 9 columns (redoxes and each species
 its frequency",length(TallySpeciesPerRedox()) == 9)
\end_layout

\begin_layout Plain Layout

assert("TallySelectedSpeciesPerRedox: must be 3 columns (redoxes, Hydrobia
 and Nereis",length(TallySelectedSpeciesPerRedox()) == 3)
\end_layout

\begin_layout Plain Layout

assert("CalcOrderednessPerDistance: must have 16 distances",length(CalcOrderedne
ssPerDistance()$dist_m) == 16)
\end_layout

\begin_layout Plain Layout

\end_layout

