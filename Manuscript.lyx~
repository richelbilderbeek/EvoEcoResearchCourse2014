GetDataCombined <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	data_combined <- merge(CreateDataRedox(),GetDataBenthosSelected(),by=c("dist_m"
,"depth_cm"),all=FALSE)
\end_layout

\begin_layout Plain Layout

	data_combined$species_name <- droplevels(data_combined$species_name)
\end_layout

\begin_layout Plain Layout

	data_combined <- data_combined[ c("redox_calib","species_name") ]
\end_layout

\begin_layout Plain Layout

	data_combined
\end_layout

\begin_layout Plain Layout

}
\end_layout

