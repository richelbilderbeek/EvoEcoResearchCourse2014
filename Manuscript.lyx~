\begin_layout Plain Layout

# Research question: Can I predict where species are from a redox potential?
\end_layout

\begin_layout Plain Layout

# Experiment: 
\end_layout

\begin_layout Plain Layout

# * Couple shallow (0-5 cm) benthos abundances with redox measurement of
 2 cm
\end_layout

\begin_layout Plain Layout

# * Couple deep (5-20 cm) benthos abundances with redox measurement of 10
 cm
\end_layout

\begin_layout Plain Layout

# Answer: 
\end_layout

\begin_layout Plain Layout

# * No 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

rm(list = ls())
\end_layout

\begin_layout Plain Layout

setwd("~/GitHubs/EvoEcoResearchCourse2014")
\end_layout

\begin_layout Plain Layout

library(reshape2)
\end_layout

\begin_layout Plain Layout

library(testit)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Create benthos data as such:
\end_layout

\begin_layout Plain Layout

#  dist_m depth_cm species_name
\end_layout

\begin_layout Plain Layout

#  2350   2        Bathyporeia_spec.
\end_layout

\begin_layout Plain Layout

#  2400   10       Nereis_diversicolor
\end_layout

\begin_layout Plain Layout

# Removes species at unknown depths
\end_layout

\begin_layout Plain Layout

CreateDataBenthos <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	data_benthos <- read.table("benthos_species_diversity.csv",header=TRUE,sep=",")
\end_layout

\begin_layout Plain Layout

	#Remove useless columns
\end_layout

\begin_layout Plain Layout

	data_benthos <-data_benthos[ c("dist_m","soil_layer","species_name") ]
\end_layout

\begin_layout Plain Layout

	# Only keep rows with 'D' or 'S' (for deep and shallow/top part of the
 core)
\end_layout

\begin_layout Plain Layout

	data_benthos<-data_benthos[data_benthos$soil_layer != "U" & data_benthos$soil_l
ayer != "?", ]
\end_layout

\begin_layout Plain Layout

	# Drop the unneeded levels 
\end_layout

\begin_layout Plain Layout

	data_benthos$soil_layer <- droplevels(data_benthos$soil_layer)
\end_layout

\begin_layout Plain Layout

	#Of data_benthod, change the column 'soil_layer' to 'depth_cm': 
\end_layout

\begin_layout Plain Layout

	# if soil_layer == "S" -> depth_cm = 2
\end_layout

\begin_layout Plain Layout

	# if soil_layer == "D" -> depth_cm = 10
\end_layout

\begin_layout Plain Layout

	names(data_benthos) <- c("dist_m","depth_cm","species_name")
\end_layout

\begin_layout Plain Layout

	data_benthos$depth_cm <- as.factor(data_benthos$depth_cm)
\end_layout

\begin_layout Plain Layout

	# Rename the levels
\end_layout

\begin_layout Plain Layout

	levels(data_benthos$depth_cm) <- c(2,10)
\end_layout

\begin_layout Plain Layout

	data_benthos
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Count the species occurring at two depths
\end_layout

\begin_layout Plain Layout

# species_name      2  10
\end_layout

\begin_layout Plain Layout

# Arenicola_marina  12  1
\end_layout

\begin_layout Plain Layout

# Bathyporeia_spec.
 2   0
\end_layout

\begin_layout Plain Layout

GetSpeciesCountAtDepths <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	species_to_depth <- dcast(
\end_layout

\begin_layout Plain Layout

		CreateDataBenthos(),
\end_layout

\begin_layout Plain Layout

		species_name ~ depth_cm,
\end_layout

\begin_layout Plain Layout

		value.var = "species_name",
\end_layout

\begin_layout Plain Layout

		fill = 0, 
\end_layout

\begin_layout Plain Layout

		fun.aggregate=length # fun.aggregate may also be mean
\end_layout

\begin_layout Plain Layout

	) 
\end_layout

\begin_layout Plain Layout

	species_to_depth
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Obtain the species names occuring at least thrice in both of the two depths
 in the soil
\end_layout

\begin_layout Plain Layout

# Will be this:
\end_layout

\begin_layout Plain Layout

# [1] Carcinus_maenas     Cerastoderma_edule  Crassostrea_gigas   Hemigropsus_ta
kanoi Hydrobia_ulvae     
\end_layout

\begin_layout Plain Layout

# [6] Littorina_littorea  Mytilus_edulis      Nereis_diversicolor
\end_layout

\begin_layout Plain Layout

# 8 Levels: Carcinus_maenas Cerastoderma_edule Crassostrea_gigas Hemigropsus_tak
anoi ...
 Nereis_diversicolor
\end_layout

\begin_layout Plain Layout

GetSelectedSpecies <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	species_to_depth <- GetSpeciesCountAtDepths()
\end_layout

\begin_layout Plain Layout

	selected_species_to_depth <- subset(species_to_depth,
\end_layout

\begin_layout Plain Layout

		  species_to_depth$"2" > 2 & species_to_depth$"10" > 2)
\end_layout

\begin_layout Plain Layout

	selected_species_to_depth <- droplevels(selected_species_to_depth)
\end_layout

\begin_layout Plain Layout

	# Collect all species' names
\end_layout

\begin_layout Plain Layout

	species_list <- levels(selected_species_to_depth$species_name)
\end_layout

\begin_layout Plain Layout

	selected_species <- selected_species_to_depth$species_name
\end_layout

\begin_layout Plain Layout

	selected_species <- droplevels(selected_species)
\end_layout

\begin_layout Plain Layout

	selected_species
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

GetDataBenthosSelected <- function()
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

  data_benthos <- CreateDataBenthos()
\end_layout

\begin_layout Plain Layout

	selected_species <- GetSelectedSpecies()
\end_layout

\begin_layout Plain Layout

  #data_benthos_selected <- data_benthos[ data_benthos$species_name %in%
 selected_species,]
\end_layout

\begin_layout Plain Layout

	data_benthos_selected <- data_benthos[ data_benthos$species_name %in% GetSelect
edSpecies(),]
\end_layout

\begin_layout Plain Layout

  data_benthos_selected
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Prepare the redox data
\end_layout

\begin_layout Plain Layout

# dist_m depth_cm replicate redox_calib
\end_layout

